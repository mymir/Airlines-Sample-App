<!DOCTYPE html>
<html>
  <head>
    <title>AirlinesSampleApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% flash.each do |type, msg| %>
      <div class="flash">
        <h2><%= msg %></h2>
      </div>
    <% end %>
    <%= yield %>
  </body>
</html>
